#!/usr/bin/env ruby

require "pry"
require_relative "../config/environment"
DB = { conn: SQLite3::Database.new("db/music.db") }

def reset_database
  Song.drop_table
  Song.create_table
  Song.create(name: "Hello", album: "25")
  Song.create(name: "99 Problems", album: "The Black Album")
end

reset_database

binding.pry
"pls"
